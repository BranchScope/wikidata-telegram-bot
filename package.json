{
	"name": "wikidata-telegram-bot",
	"private": true,
	"version": "1.3.3",
	"description": "Quick Look on Wikidata Entities via Telegram",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-telegram-bot",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "edjopato.de"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"start": "ts-node source/index.ts",
		"test": "tsc && xo"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [
		"wikidata",
		"telegram"
	],
	"dependencies": {
		"array-filter-unique": "^2.1.0",
		"got": "^10.0.4",
		"locale-emoji": "^0.3.0",
		"source-map-support": "^0.5.12",
		"telegraf": "^3.30.1",
		"telegraf-i18n": "^6.5.0",
		"telegraf-inline-menu": "^4.0.0",
		"telegraf-session-local": "^1.0.2",
		"telegraf-wikibase": "^0.4.0",
		"telegram-format": "^1.0.0",
		"wikidata-entity-reader": "^1.3.0",
		"wikidata-entity-store": "^2.0.1",
		"wikidata-sdk": "^6.7.0",
		"wikidata-sdk-got": "^1.1.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/node": "^13.7.1",
		"@typescript-eslint/eslint-plugin": "^2.19.2",
		"@typescript-eslint/parser": "^2.19.2",
		"del-cli": "^3.0.0",
		"eslint-config-xo-typescript": "^0.26.0",
		"telegram-typings": "^4.0.0-beta",
		"ts-node": "^8.3.0",
		"typescript": "^3.7.3",
		"xo": "^0.26.1"
	},
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"rules": {
			"prefer-named-capture-group": "off",
			"@typescript-eslint/restrict-template-expressions": "off"
		}
	}
}
